<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".FragInfoLaboral">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="30sp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="40sp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/id"/>
                <TextView
                    android:id="@+id/txv_laboral_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="16sp"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/nombre"/>
                <TextView
                    android:id="@+id/txv_laboral_nombre"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/apellido"/>
                <TextView
                    android:id="@+id/txv_laboral_apellido"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/nomina"/>
                <TextView
                    android:id="@+id/txv_nomina"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/area" />

                <TextView
                    android:id="@+id/txv_area"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/puesto"/>
                <TextView
                    android:id="@+id/txv_puesto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/curp"/>

                <TextView
                    android:id="@+id/txv_curp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/nss"/>
                <TextView
                    android:id="@+id/txv_nss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/contEmergencia"/>
                <TextView
                    android:id="@+id/txv_contacto_emergencia"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/escolaridad"/>
                <TextView
                    android:id="@+id/txv_escolaridad"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:text="@string/status"/>
                <TextView
                    android:id="@+id/txv_status"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_marginHorizontal="10sp"
                    android:textSize="18sp"
                    android:textColor="@color/gray"
                    android:layout_weight="1"/>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</LinearLayout>